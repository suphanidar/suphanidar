# Frontend Environment Variables

REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=http://localhost:5000
REACT_APP_MODEL_PATH=/ai-models/yolov8-waste-model.json
REACT_APP_APP_NAME=WasteSortAI
REACT_APP_VERSION=1.0.0

# AI Model Configuration

REACT_APP_MODEL_CONFIDENCE_THRESHOLD=0.5
REACT_APP_MODEL_NMS_THRESHOLD=0.4
REACT_APP_MODEL_MAX_DETECTIONS=100

# Camera Configuration

REACT_APP_CAMERA_WIDTH=1280
REACT_APP_CAMERA_HEIGHT=720
REACT_APP_IMAGE_QUALITY=0.8
REACT_APP_MAX_FILE_SIZE=10485760

# Feature Flags

REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Development

REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Production Settings (comment out for development)

# REACT_APP_API_URL=https://api.wastesort.ai/api

# REACT_APP_SOCKET_URL=https://api.wastesort.ai

# REACT_APP_DEBUG=false

# REACT_APP_LOG_LEVEL=error

# External Services

REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=
REACT_APP_FIREBASE_CONFIG=

# CDN URLs

REACT_APP_ASSETS_CDN=
REACT_APP_MODELS_CDN=

# Localization

REACT_APP_DEFAULT_LANGUAGE=th
REACT_APP_SUPPORTED_LANGUAGES=th,en

# Performance

REACT_APP_BUNDLE_ANALYZER=false
REACT_APP_LAZY_LOADING=true