{
“name”: “wastesort-ai-backend”,
“version”: “1.0.0”,
“description”: “WasteSortAI Backend API with Node.js, Express, MongoDB, and TensorFlow.js”,
“main”: “server.js”,
“scripts”: {
“start”: “node server.js”,
“dev”: “nodemon server.js”,
“test”: “jest –watchAll=false”,
“test:watch”: “jest –watchAll”,
“test:coverage”: “jest –coverage –watchAll=false”,
“setup-db”: “node scripts/setup-database.js”,
“seed-data”: “node scripts/seed-data.js”,
“migrate”: “node scripts/migrate.js”,
“backup”: “node scripts/backup.js”,
“lint”: “eslint . –fix”,
“format”: “prettier –write .”,
“build”: “echo ‘No build step required for Node.js’”,
“docker:build”: “docker build -t wastesort-ai-backend .”,
“docker:run”: “docker run -p 5000:5000 wastesort-ai-backend”,
“deploy”: “pm2 start ecosystem.config.js –env production”,
“logs”: “pm2 logs wastesort-ai”,
“monitor”: “pm2 monit”,
“health-check”: “curl -f http://localhost:5000/api/health || exit 1”
},
“keywords”: [
“waste-sorting”,
“ai”,
“computer-vision”,
“tensorflow”,
“recycling”,
“environmental”,
“nodejs”,
“express”,
“mongodb”
],
“author”: “WasteSortAI Team”,
“license”: “MIT”,
“engines”: {
“node”: “>=18.0.0”,
“npm”: “>=8.0.0”
},
“dependencies”: {
“express”: “^4.18.2”,
“mongoose”: “^8.0.3”,
“bcryptjs”: “^2.4.3”,
“jsonwebtoken”: “^9.0.2”,
“multer”: “^1.4.5-lts.1”,
“cors”: “^2.8.5”,
“helmet”: “^7.1.0”,
“express-rate-limit”: “^7.1.5”,
“joi”: “^17.11.0”,
“socket.io”: “^4.7.4”,
“@tensorflow/tfjs-node”: “^4.15.0”,
“sharp”: “^0.32.6”,
“nodemailer”: “^6.9.7”,
“redis”: “^4.6.11”,
“express-validator”: “^7.0.1”,
“compression”: “^1.7.4”,
“morgan”: “^1.10.0”,
“dotenv”: “^16.3.1”,
“cookie-parser”: “^1.4.6”,
“express-session”: “^1.17.3”,
“connect-mongo”: “^5.1.0”,
“passport”: “^0.7.0”,
“passport-local”: “^1.0.0”,
“passport-jwt”: “^4.0.1”,
“express-async-errors”: “^3.1.1”,
“winston”: “^3.11.0”,
“winston-daily-rotate-file”: “^4.7.1”,
“node-cron”: “^3.0.3”,
“archiver”: “^6.0.1”,
“csv-writer”: “^1.6.0”,
“pdf-kit”: “^0.14.0”,
“qrcode”: “^1.5.3”,
“nodemailer-smtp-transport”: “^2.7.4”,
“express-slow-down”: “^1.6.0”,
“express-brute”: “^1.0.1”,
“express-brute-redis”: “^0.0.1”,
“http-status-codes”: “^2.3.0”,
“uuid”: “^9.0.1”,
“mime-types”: “^2.1.35”,
“slugify”: “^1.6.6”,
“moment”: “^2.29.4”,
“lodash”: “^4.17.21”,
“await-to-js”: “^3.0.0”
},
“devDependencies”: {
“nodemon”: “^3.0.2”,
“jest”: “^29.7.0”,
“supertest”: “^6.3.3”,
“eslint”: “^8.55.0”,
“eslint-config-node”: “^4.1.0”,
“eslint-plugin-node”: “^11.1.0”,
“prettier”: “^3.1.0”,
“husky”: “^8.0.3”,
“lint-staged”: “^15.2.0”,
“@types/jest”: “^29.5.8”,
“mongodb-memory-server”: “^9.1.3”,
“cross-env”: “^7.0.3”,
“concurrently”: “^8.2.2”
},
“jest”: {
“testEnvironment”: “node”,
“coverageDirectory”: “coverage”,
“collectCoverageFrom”: [
“**/*.js”,
“!node_modules/**”,
“!coverage/**”,
“!jest.config.js”
],
“testMatch”: [
“**/**tests**/**/*.js”,
“**/?(*.)+(spec|test).js”
],
“setupFilesAfterEnv”: [
“./tests/setup.js”
]
},
“eslintConfig”: {
“extends”: [
“eslint:recommended”,
“node”
],
“env”: {
“node”: true,
“es2021”: true,
“jest”: true
},
“rules”: {
“no-console”: “warn”,
“no-unused-vars”: “error”,
“prefer-const”: “error”
}
},
“prettier”: {
“semi”: true,
“trailingComma”: “es5”,
“singleQuote”: true,
“printWidth”: 80,
“tabWidth”: 2
},
“husky”: {
“hooks”: {
“pre-commit”: “lint-staged”
}
},
“lint-staged”: {
“*.js”: [
“eslint –fix”,
“prettier –write”,
“git add”
]
},
“repository”: {
“type”: “git”,
“url”: “https://github.com/wastesort-ai/backend.git”
},
“bugs”: {
“url”: “https://github.com/wastesort-ai/backend/issues”
},
“homepage”: “https://wastesort.ai”
}